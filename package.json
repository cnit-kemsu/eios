{
  "scripts": {
    "start": "npx webpack-dev-server --mode development",
    "build:dev": "npx webpack --mode development",
    "build:prod": "npx webpack --mode production",
    "lint": "npx eslint ./src",
    "git:pull": "git pull origin master",
    "git:push": "git push origin HEAD:master",
    "deploy": "npx run-s git:pull git:push \"build:prod -- --deploy\""
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@hot-loader/react-dom": "^16.11.0",
    "@kemsu/eios-ui": "^0.2.0",
    "@kemsu/react-routing": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.15",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@emotion/babel-preset-css-prop": "^10.0.22",
    "@types/jest": "^24.0.19",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-react": "^7.16.0",
    "fast-async": "^6.3.8",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "babelOptions": {
    "babelrc": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "exclude": [
            "transform-async-to-generator",
            "transform-regenerator"
          ]
        }
      ],
      "@babel/preset-react",
      "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
      [
        "module:fast-async",
        {
          "spec": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ]
    ]
  },
  "author": {
    "name": "Igor Sotnikov",
    "email": "sotnikov@kemsu.ru"
  }
}
